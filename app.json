{
  "name": "iconic_chatbot",
  "description": "A telegram CHAT bot",
  "logo": "https://te.legra.ph/file/cdec4aaef86bef52224c7.jpg",
  "keywords": [
    "Chatbot",
    "mongodb",
    "python",
    "pyrogram"
  ],
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "repository": "https://github.com/Vasi420/ANNIE-X-CHATBOT",
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API hash",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot token from @BotFather",
      "required": true
    },
    "MONGO_URL": {
      "description": "Get a mongodb url from https://cloud.mongodb.com.",
      "required": true
    },
    "OWNER_ID": {
      "description": "Your Telegram user ID (Owner ID)",
      "required": true
    },
    "OWNER_USERNAME": {
      "description": "Username of the bot owner",
      "required": true,
      "value": "Anbesivam_Owner"
    },
    "GPT_API": {
      "description": "Your GPT API Key from OpenAI.",
      "value": "",
      "required": true
    },
    "SUPPORT_GROUP": {
      "description": "Name of the support group",
      "required": true,
      "value": "asbotshub"
    },
    "UPDATE_CHANNEL": {
      "description": "Name of the update channel",
      "required": true,
      "value": "Anbe_Sivam_Bots"
    }
  }
}
